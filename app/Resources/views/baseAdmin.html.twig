    $('.odd').on('click', function(){
        oddid = $(this).data('id');
    });
    loader = "<div id='loader' class='spinner spinner-primary spinner-lg' style='position:relative;min-height:60px;display: none;'></div>";
    $('.content-modal').append(loader);


        $('#fb_betbundle').on('submit', function(e) {
            e.preventDefault();

            var $this = $(this);
                $.ajax({
                    url: window.location+'bet/new/'+oddid,
                    type: $this.attr('method'),
                    data:  "&mise="+$this.find('#miseUser').val(),
                    beforeSend: function() {
                        $('#fb_betbundle').hide();
                        $('#loader').show();
                    },
                    complete: function(){
                        $('#fb_betbundle').show();
                        $('#loader').hide();
                    },
                    success: function() {
                        $('#infoModalColoredHeader').modal('hide');

                    },
                    error: function(){
